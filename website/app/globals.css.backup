/* Tab Visual Container Styles */
.tab-visual__wrap {
  position: relative;
  width: 100%;
  min-height: 500px;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.tab-visual__item {
  width: 100%;
  height: 100%;
  min-height: 500px;
  transition: opacity 0.5s ease, visibility 0.5s ease;
}

.tab-visual__item.active {
  z-index: 1;
}

.tab-image {
  width: 100%;
  height: 100%;
  min-height: 500px;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
}

/* Tab Content Styles */
.tab-content__item {
  display: block;
  text-align: left;
  padding: 1rem 0;
  border: none;
  background: transparent;
  cursor: pointer;
  transition: all 0.3s ease;
  width: 100%;
  position: relative;
  border-bottom: 1px solid rgba(255, 255, 255, 0.1);
}

.tab-content__item:hover {
  opacity: 0.8;
}

.tab-content__item.active .tab-content__item-detail {
  opacity: 1;
  max-height: 200px;
}

.tab-content__item-main {
  margin-bottom: 0.5rem;
}

.tab-content__item-detail {
  overflow: hidden;
  transition: all 0.3s ease;
  opacity: 0.7;
  max-height: 0;
}

/* Section Styles */
.home_product.section {
  padding: 80px 0;
  min-height: 100vh;
  display: flex;
  align-items: center;
}

@media (max-width: 768px) {
  .tab-visual__wrap,
  .tab-visual__item,
  .tab-image {
    min-height: 300px;
  }

  .home_product.section {
    padding: 40px 0;
    min-height: auto;
  }
}

